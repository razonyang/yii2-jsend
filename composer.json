{
    "name": "razonyang/yii2-jsend",
    "description": "JSend port for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","jsend"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Razon Yang",
            "email": "razonyang@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "razonyang/jsend": "^1.0",
        "yiisoft/yii2": "~2.0.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "RazonYang\\Yii2\\JSend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RazonYang\\Yii2\\JSend\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
